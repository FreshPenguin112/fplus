{
  "name": "fplus",
  "version": "0.1.0",
  "bin": {
    "fplus": "index.js"
  },
  "scripts": {
    "build": "npm i -g bun && bun i && antlr4ng -o ./lib -Dlanguage=JavaScript -visitor -listener *.g4 && babel ./lib --out-dir ./lib --plugins=@babel/plugin-transform-modules-commonjs",
    "pkg": "npm run build && pkg --targets node18-linux-x64,node18-win-x64,node18-macos-x64 --out-path ./dist index.js"
  },
  "dependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "antlr4": "^4.13.1-patch-1",
    "antlr4ng": "^3.0.4",
    "cli-highlight": "^2.1.11",
    "pkg": "^5.8.1",
    "uglify-js": "^3.17.4"
  }
}
